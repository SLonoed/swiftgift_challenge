{
  "name": "swiftgift_challenge",
  "version": "1.0.0",
  "description": "SwiftGift code challenge",
  "scripts": {
    "install": "./node_modules/.bin/bower i && ./node_modules/.bin/webdriver-manager update --standalone",
    "test-server": "./node_modules/.bin/webdriver-manager start",
    "serve": "node_modules/.bin/http-server dist/ -p 8081 -o",
    "dist": "NODE_ENV=production ./node_modules/.bin/webpack -p",
    "dev": "./node_modules/.bin/webpack-dev-server --devtool eval --progress --colors --hot --content-base dist/ --inline",
    "test": "./node_modules/.bin/protractor protractor.js"
  },
  "author": "Dmitry Manannikov <slonoed@gmail.com> (http://slonoed.net/)",
  "devDependencies": {
    "bower": "1.3.12",
    "css-loader": "0.9.1",
    "file-loader": "0.8.1",
    "font-awesome": "4.2.0",
    "font-awesome-webpack": "0.0.2",
    "jsx-loader": "0.12.2",
    "less": "2.2.0",
    "less-loader": "2.0.0",
    "protractor": "1.6.1",
    "selenium-webdriver": "2.44.0",
    "style-loader": "0.8.3",
    "url-loader": "0.5.5",
    "webpack": "1.5.1",
    "webpack-dev-server": "1.7.0"
  },
  "dependencies": {
    "http-server": "0.7.4"
  }
}
